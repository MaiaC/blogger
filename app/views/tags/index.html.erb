<h1>All Tags</h1>

<ul id="tags">
    <% @tags.each do |tag| %>
      <li>
        <%= link_to tag.name, tag_path(tag), class: 'tag_name' %>
      </li>
    <% end %>
  </ul>
  
  <%= link_to "Create a New Article", new_article_path(@article), class: "new_article" %>
  <%= link_to "View Article List", articles_path %>